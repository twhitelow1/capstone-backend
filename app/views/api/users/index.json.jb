@users.map do |user|
  {
    id: user.id,
    email: user.email,    
    first_name: user.first_name,
    last_name: user.last_name,
    home: user.home,
    points: user.points,
    profile_img_url: user.profile_img_url,
    housemates: user.housemates,
    full_name: "#{user.first_name.capitalize} #{user.last_name.capitalize}"


  }
end